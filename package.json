{
  "name": "reactvscode",
  "displayName": "React Vscode Framework",
  "description": "",
  "publisher": "reactvscodepublisher",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "reactvscode-explorer",
          "title": "ReactVscode Explorer",
          "icon": "src/constants/assets/logo.svg"
        }
      ]
    },
    "views": {
      "reactvscode-explorer": [
        {
          "id": "basicwebview",
          "name": "Local Storage Demo",
          "type": "webview"
        },
        {
          "id": "tstree",
          "name": "TS Tree View",
          "type": "tree"
        },
        {
          "id": "jsontree",
          "name": "JSON Tree View",
          "type": "tree"
        }
      ]
    },
    "commands": [
      {
        "command": "reactvscode.helloWorld",
        "title": "Hello World",
        "category": "RVF:COMMANDS"
      },
      {
        "command": "reactvscode.welcomewebview",
        "title": "Welcome Page",
        "category": "RVF:WEB"
      },
      {
        "command": "reactvscode.tstree",
        "title": "Type Trees",
        "category": "RVF:TREE"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.6",
    "@types/glob": "^8.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "html-webpack-plugin": "^5.5.0",
    "loadash": "^1.0.0",
    "mocha": "^10.1.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}